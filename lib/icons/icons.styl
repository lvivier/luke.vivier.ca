@font-face
  font-family: 'icons'
  src:url('icons.eot')
  src:url('icons.eot?#iefix') format('embedded-opentype'),
    url('icons.woff') format('woff'),
    url('icons.ttf') format('truetype'),
    url('icons.svg#icons') format('svg')
  font-weight: normal
  font-style: normal

[data-icon]:before
  font-family: 'icons'
  content: attr(data-icon)
  speak: none
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1
  -webkit-font-smoothing: antialiased
