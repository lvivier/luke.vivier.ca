@font-face
  font-family: 'vivier'
  src:url('fonts/vivier.eot')
  src:url('fonts/vivier.eot?#iefix') format('embedded-opentype'),
    url('fonts/vivier.woff') format('woff'),
    url('fonts/vivier.ttf') format('truetype'),
    url('fonts/vivier.svg#vivier') format('svg')
  font-weight: normal
  font-style: normal

[data-icon]:before
  font-family: 'vivier'
  content: attr(data-icon)
  speak: none
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1
  -webkit-font-smoothing: antialiased
